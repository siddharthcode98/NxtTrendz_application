#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/bin/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/tailwindcss@3.3.3/node_modules/tailwindcss/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/tailwindcss@3.3.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/bin/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules/sucrase/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/sucrase@3.34.0/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/tailwindcss@3.3.3/node_modules/tailwindcss/node_modules:/home/workspace/reactjs/coding-practices/NxtTrendzEcommerceApp/myapp/node_modules/.pnpm/tailwindcss@3.3.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sucrase@3.34.0/node_modules/sucrase/bin/sucrase" "$@"
else
  exec node  "$basedir/../../../../../sucrase@3.34.0/node_modules/sucrase/bin/sucrase" "$@"
fi
