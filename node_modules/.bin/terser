#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules/terser/bin/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules/terser/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/terser/bin/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/terser/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules/terser/bin/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules/terser/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/terser@5.19.4/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/terser/bin/node_modules:/home/workspace/reactjs/coding-practices/EcommerceDuplicate/myapp/node_modules/terser/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
